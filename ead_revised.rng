<grammar xmlns="http://relaxng.org/ns/structure/1.0" ns="http://ead3.archivists.org/schema/"
	datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
	
	<start>
		<ref name="e.ead"/>
	</start>
	
	<!-- ELEMENTS -->
	
	<define name="e.ead">
		<element name="ead">
			<ref name="a.common"/>
			<ref name="a.relatedencoding"/>
			
			<ref name="e.control"/>
			
			<element name="archdesc">
				<ref name="a.common"/>
				<ref name="a.localtype"/>
				<ref name="a.relatedencoding"/>
				<ref name="a.desc.base"/>
				<ref name="a.level"/>
				<ref name="e.did"/>
				<zeroOrMore>
					<ref name="m.desc.base.full"/>
				</zeroOrMore>
			</element>
		</element>
	</define>
	
	<!-- control -->

	<define name="e.control">
		<element name="control">
			<ref name="a.common"/>
			<ref name="a.encodinganalog"/>
			<ref name="a.relatedencoding"/>
			<ref name="a.base"/>
			<optional>
				<attribute name="langencoding">
					<choice>
						<value>iso639-1</value>
						<value>iso639-2b</value>
						<value>iso639-3</value>
						<value>otherlangencoding</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="scriptencoding">
					<choice>
						<value>iso15924</value>
						<value>otherscriptencoding</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="dateencoding">
					<choice>
						<value>iso8601</value>
						<value>otherdateencoding</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="countryencoding">
					<choice>
						<value>iso3166-1</value>
						<value>othercountryencoding</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="repositoryencoding">
					<choice>
						<value>iso15511</value>
						<value>otherrepositoryencoding</value>
					</choice>
				</attribute>
			</optional>
			
			<element name="recordid">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				<optional>
					<attribute name="instanceurl">
						<data type="anyURI"/>
					</attribute>
				</optional>
				<text/>
			</element>
			
			<zeroOrMore>
				<element name="otherrecordid">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<text/>
				</element>
			</zeroOrMore>
			
			<zeroOrMore>
				<element name="representation">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="am.simplelink"/>
					<ref name="a.localtype"/>
					<text/>
				</element>
			</zeroOrMore>
			
			<element name="filedesc">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				
				<element name="titlestmt">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<oneOrMore>
						<ref name="e.titleproper"/>
					</oneOrMore>
					<zeroOrMore>
						<ref name="e.subtitle"/>
					</zeroOrMore>
					<zeroOrMore>
						<ref name="e.author"/>
					</zeroOrMore>
					<zeroOrMore>
						<ref name="e.sponsor"/>
					</zeroOrMore>
				</element>
				
				<optional>
					<element name="editionstmt">
						<ref name="a.common"/>
						<ref name="a.encodinganalog"/>
						<oneOrMore>
							<choice>
								<ref name="e.edition"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				
				<optional>
					<element name="publicationstmt">
						<ref name="a.common"/>
						<ref name="a.encodinganalog"/>
						<oneOrMore>
							<choice>
								<ref name="e.publisher"/>
								<ref name="e.date"/>
								<ref name="e.address"/>
								<ref name="e.num"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				
				<optional>
					<element name="seriesstmt">
						<ref name="a.common"/>
						<ref name="a.encodinganalog"/>
						<oneOrMore>
							<choice>
								<ref name="e.titleproper"/>
								<ref name="e.num"/>
								<ref name="e.p"/>
							</choice>
						</oneOrMore>
					</element>
				</optional>
				
				<optional>
					<element name="notestmt">
						<ref name="a.common"/>
						<ref name="a.encodinganalog"/>
						<oneOrMore>
							<ref name="e.controlnote"/>
						</oneOrMore>
					</element>
				</optional>
			</element>
			
			<element name="maintenancestatus">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				<attribute name="value">
					<choice>
						<value>revised</value>
						<value>deleted</value>
						<value>new</value>
						<value>deletedsplit</value>
						<value>deletedmerged</value>
						<value>deletedreplaced</value>
						<value>cancelled</value>
						<value>derived</value>
					</choice>
				</attribute>
				<text/>
			</element>
			
			<optional>
				<element name="publicationstatus">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<attribute name="value">
						<choice>
							<value>inprocess</value>
							<value>approved</value>
							<value>published</value>
						</choice>
					</attribute>
					<text/>
				</element>
			</optional>
			
			<ref name="e.maintenanceagency"/>
			
			<zeroOrMore>
				<element name="languagedeclaration">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="e.language"/>
					<ref name="e.script"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>
			</zeroOrMore>
			
			<zeroOrMore>
				<element name="conventiondeclaration">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<optional>
						<ref name="e.abbr"/>
					</optional>
					<ref name="e.citation"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>
			</zeroOrMore>
			
			<zeroOrMore>
				<element name="localtypedeclaration">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<optional>
						<ref name="e.abbr"/>
					</optional>
					<ref name="e.citation"/>
					<optional>
						<ref name="e.descriptivenote"/>
					</optional>
				</element>
			</zeroOrMore>
			
			<zeroOrMore>
				<element name="localcontrol">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<optional>
						<element name="term">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<ref name="a.transliteration"/>
							<optional>
								<ref name="a.lastdatetimeverified"/>
							</optional>
							<ref name="a.access.no.normal"/>
							<text/>
						</element>
					</optional>
					<optional>
						<choice>
							<ref name="e.datesingle"/>
							<ref name="e.daterange"/>
						</choice>
					</optional>
				</element>
			</zeroOrMore>
			
			<element name="maintenancehistory">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				<oneOrMore>
					<element name="maintenanceevent">
						<ref name="a.common"/>
						<ref name="a.encodinganalog"/>
						
						<element name="eventtype">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<attribute name="value">
								<choice>
									<value>created</value>
									<value>revised</value>
									<value>deleted</value>
									<value>cancelled</value>
									<value>derived</value>
									<value>updated</value>
									<value>unknown</value>
								</choice>
							</attribute>
							<text/>
						</element>
						
						<element name="eventdatetime">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<optional>
								<attribute name="standarddatetime">
									<data type="token"/>
								</attribute>
							</optional>
							<text/>
						</element>
						
						<element name="agenttype">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<attribute name="value">
								<choice>
									<value>human</value>
									<value>machine</value>
									<value>unknown</value>
								</choice>
							</attribute>
							<text/>
						</element>
						
						<element name="agent">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<text/>
						</element>
						
						<zeroOrMore>
							<element name="eventdescription">
								<ref name="a.common"/>
								<ref name="a.encodinganalog"/>
								<ref name="a.localtype"/>
								<text/>
							</element>
						</zeroOrMore>
					</element>
				</oneOrMore>
			</element>
			
			<optional>
				<element name="sources">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<ref name="a.base"/>
					
					<oneOrMore>
						<element name="source">
							<ref name="a.common"/>
							<ref name="a.encodinganalog"/>
							<optional>
								<ref name="a.lastdatetimeverified"/>
							</optional>
							<ref name="am.simplelink"/>
							<zeroOrMore>
								<element name="sourceentry">
									<ref name="a.common"/>
									<ref name="a.encodinganalog"/>
									<ref name="a.transliteration"/>
									<text/>
								</element>
							</zeroOrMore>
							<optional>
								<ref name="e.objectxmlwrap"/>
							</optional>
							<optional>
								<ref name="e.descriptivenote"/>
							</optional>
						</element>
					</oneOrMore>
				</element>
			</optional>
		</element>
	</define>

	<define name="e.titleproper">
		<element name="titleproper">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.render"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.subtitle">
		<element name="subtitle">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.author">
		<element name="author">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.sponsor">
		<element name="sponsor">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.edition">
		<element name="edition">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>

	<define name="e.publisher">
		<element name="publisher">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>
	
	<define name="e.controlnote">
		<element name="controlnote">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<oneOrMore>
				<ref name="m.blocks"/>
			</oneOrMore>
		</element>
	</define>

	<define name="e.maintenanceagency">
		<element name="maintenanceagency">
			<ref name="a.common"/>
			<ref name="a.encodinganalog"/>
			<ref name="am.countrycode"/>
			
			<optional>
				<element name="agencycode">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<text/>
				</element>
			</optional>
			
			<zeroOrMore>
				<element name="otheragencycode">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<text/>
				</element>
			</zeroOrMore>
			
			<oneOrMore>
				<element name="agencyname">
					<ref name="a.common"/>
					<ref name="a.encodinganalog"/>
					<ref name="a.localtype"/>
					<text/>
				</element>
			</oneOrMore>
			
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>
	</define>

	<define name="e.citation">
		<element name="citation">
			<ref name="a.common"/>
			<ref name="a.encodinganalog"/>
			<ref name="am.simplelink"/>
			<optional>
				<ref name="a.lastdatetimeverified"/>
			</optional>
			<ref name="m.mixed.basic"/>
		</element>
	</define>

	<!-- did -->

	<define name="e.did">
		<element name="did">
			<ref name="a.common"/>
			<ref name="a.encodinganalog"/>
			<optional>
				<ref name="e.head"/>
			</optional>
			<oneOrMore>
				<ref name="m.did"/>
			</oneOrMore>
		</element>
	</define>
	
	<define name="e.dao">
		<element name="dao">
			<ref name="a.common"/>
			<ref name="a.encodinganalog"/>
			<ref name="a.localtype"/>
			<ref name="a.label"/>
			<ref name="am.simplelink"/>
			<ref name="a.identifier"/>
			<ref name="a.link.xpointer"/>
			<optional>
				<attribute name="entityref">
					<data type="ENTITY"/>
				</attribute>
			</optional>
			<attribute name="daotype">
				<choice>
					<value>borndigital</value>
					<value>derived</value>
					<value>unknown</value>
					<value>otherdaotype</value>
				</choice>
			</attribute>
			<optional>
				<attribute name="otherdaotype"/>
			</optional>
			<optional>
				<ref name="a.coverage"/>
			</optional>
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>
	</define>
	
	<define name="e.daoset">
		<element name="daoset">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.encodinganalog"/>
			<ref name="a.label"/>
			<ref name="a.coverage"/>
			<ref name="e.dao"/>
			<oneOrMore>
				<ref name="e.dao"/>
			</oneOrMore>
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>
	</define>
	
	<define name="e.materialspec">
		<element name="materialspec">
			<ref name="a.common"/>
			<ref name="a.label"/>
			<ref name="a.encodinganalog"/>
			<ref name="a.localtype"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>

	<define name="e.origination">
		<element name="origination">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.label"/>
			<ref name="a.encodinganalog"/>
			<oneOrMore>
				<choice>
					<ref name="e.corpname"/>
					<ref name="e.famname"/>
					<ref name="e.name"/>
					<ref name="e.persname"/>
				</choice>
			</oneOrMore>
		</element>
	</define>

	<define name="e.physdescstructured">
		<element name="physdescstructured">
			<ref name="a.common"/>
			<ref name="a.label"/>
			<ref name="a.encodinganalog"/>
			<attribute name="physdescstructuredtype">
				<choice>
					<value>carrier</value>
					<value>materialtype</value>
					<value>spaceoccupied</value>
					<value>otherphysdescstructuredtype</value>
				</choice>
			</attribute>
			<optional>
				<attribute name="otherphysdescstructuredtype"/>
			</optional>
			<ref name="a.coverage"/>
			<element name="quantity">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				<optional>
					<attribute name="approximate">
						<ref name="av.yesorno"/>
					</attribute>
				</optional>
				<text/>
			</element>
			<element name="unittype">
				<ref name="a.common"/>
				<ref name="a.encodinganalog"/>
				<ref name="a.access.no.normal"/>
				<text/>
			</element>
			<zeroOrMore>
				<choice>
					<ref name="e.physfacet"/>
					<ref name="e.dimensions"/>
				</choice>
			</zeroOrMore>
			<optional>
				<ref name="e.descriptivenote"/>
			</optional>
		</element>
	</define>

	<define name="e.repository">
		<element name="repository">
			<ref name="a.common"/>
			<ref name="a.localtype"/>
			<ref name="a.label"/>
			<ref name="a.encodinganalog"/>
			<oneOrMore>
				<choice>
					<ref name="e.corpname"/>
					<ref name="e.famname"/>
					<ref name="e.name"/>
					<ref name="e.persname"/>
				</choice>
			</oneOrMore>
			<optional>
				<ref name="e.address"/>
			</optional>
		</element>
	</define>

	<define name="e.unitdate">
		<element name="unitdate">
			<ref name="a.common"/>
			<ref name="a.label"/>
			<ref name="a.unitdatetype"/>
			<ref name="a.datechar"/>
			<ref name="a.certainty"/>
			<ref name="am.dates.era"/>
			<ref name="am.dates.calendar"/>
			<ref name="am.date.normal"/>
			<ref name="a.encodinganalog"/>
			<ref name="m.mixed.basic"/>
		</element>
	</define>

	<define name="e.unitdatestructured">
		<element name="unitdatestructured">
			<ref name="a.common"/>
			<ref name="a.label"/>
			<ref name="a.unitdatetype"/>
			<ref name="a.datechar"/>
			<ref name="a.certainty"/>
			<ref name="a.encodinganalog"/>
			<ref name="am.dates.era"/>
			<ref name="am.dates.calendar"/>
			<choice>
				<ref name="e.datesingle"/>
