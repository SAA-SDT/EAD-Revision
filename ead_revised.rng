				<ref name="a.localtype"/>
				<ref name="a.label"/>
				<ref name="am.countrycode"/>
				<optional>
					<attribute name="repositorycode">
						<data type="token"/>
					</attribute>
				</optional>
				<ref name="a.identifier"/>
				<ref name="a.encodinganalog"/>
				<ref name="m.mixed.basic"/>
			</element>

			<ref name="e.unittitle"/>

		</choice>
	</define>
	
	<define name="m.refs">
		<choice>
			<ref name="e.ptr"/>
			<ref name="e.ref"/>
		</choice>
	</define>
	
	<!-- MIXED CONTENT MODELS -->

	<define name="m.mixed.basic">
		<zeroOrMore>
			<choice>
				<text/>
				<ref name="e.abbr"/>
				<ref name="e.emph"/>
				<ref name="e.expan"/>
				<ref name="e.foreign"/>
				<ref name="e.lb"/>
				<ref name="e.ptr"/>
				<ref name="e.ref"/>
			</choice>
		</zeroOrMore>
	</define>

	<define name="m.mixed.basic.plus">
		<zeroOrMore>
			<choice>
				<ref name="m.mixed.basic"/>
				<ref name="e.date"/>
				<ref name="e.footnote"/>
				<ref name="e.num"/>
				<ref name="e.quote"/>
			</choice>
		</zeroOrMore>
	</define>

	<define name="m.mixed.basic.plus.access">
		<zeroOrMore>
			<choice>
				<ref name="m.access"/>
				<ref name="m.mixed.basic"/>
				<ref name="e.date"/>
				<ref name="e.footnote"/>
				<ref name="e.num"/>
				<ref name="e.quote"/>
			</choice>
		</zeroOrMore>
	</define>
	
	<define name="m.para.content">
		<zeroOrMore>
			<choice>
				<ref name="m.mixed.basic.plus.access"/>
				<ref name="e.list"/>
			</choice>
		</zeroOrMore>
	</define>
	
	<!-- ATTRIBUTE MODELS -->

	<define name="am.internal.ptr">
		<optional>
			<attribute name="target">
				<data type="IDREF"/>
			</attribute>
		</optional>
		<ref name="a.link.xpointer"/>
		<ref name="am.simplelink"/>
	</define>

	<define name="am.simplelink">
		<optional>
			<ref name="a.link.href"/>
		</optional>
		<optional>
			<ref name="a.link.role"/>
		</optional>
		<optional>
			<ref name="a.link.arcrole"/>
		</optional>
		<optional>
			<ref name="a.link.title"/>
		</optional>
		<optional>
			<ref name="a.link.show"/>
		</optional>
		<optional>
			<ref name="a.link.actuate"/>
		</optional>
	</define>
	
	<define name="a.common.empty">
		<optional>
			<attribute name="id">
				<data type="ID"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="altrender"/>
		</optional>
		<optional>
			<attribute name="audience">
				<choice>
					<value>external</value>
					<value>internal</value>
				</choice>
			</attribute>
		</optional>
	</define>

	<define name="a.common">
		<ref name="a.common.empty"/>
		<optional>
			<attribute name="lang"/>
		</optional>
		<optional>
			<attribute name="script"/>
		</optional>
	</define>

	<define name="a.desc.base">
		<optional>
			<attribute name="otherlevel">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
		<ref name="a.encodinganalog"/>
	</define>

	<define name="a.desc.c">
		<ref name="a.common"/>
		<optional>
			<ref name="a.level"/>
		</optional>
		<ref name="a.desc.base"/>
	</define>

	<define name="a.access">
		<ref name="a.access.no.normal"/>
		<optional>
			<attribute name="normal"/>
		</optional>
	</define>

	<define name="a.access.no.normal">
		<optional>
			<attribute name="source">
				<data type="token"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="rules">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
		<ref name="a.identifier"/>
	</define>
	
	<!-- ATTRIBUTE DEFINITIONS -->
	
	<!-- xlink-modelled attributes -->
	
	<define name="a.link.actuate">
		<attribute name="actuate">
			<choice>
				<value>onload</value>
				<value>onrequest</value>
				<value>other</value>
				<value>none</value>
			</choice>
		</attribute>
	</define>
	
	<define name="a.link.arcrole">
		<attribute name="arcrole">
			<data type="anyURI"/>
		</attribute>
	</define>
	
	<define name="a.link.href">
		<attribute name="href">
			<data type="anyURI"/>
		</attribute>
	</define>
	
	<define name="a.link.role">
		<attribute name="linkrole">
			<data type="anyURI"/>
		</attribute>
	</define>
	
	<define name="a.link.show">
		<attribute name="show">
			<choice>
				<value>new</value>
				<value>replace</value>
				<value>embed</value>
				<value>other</value>
				<value>none</value>
			</choice>
		</attribute>
	</define>
	
	<define name="a.link.title">
		<attribute name="linktitle"/>
	</define>
	
	<!-- other linking-related attributes-->
	
	<define name="a.base">
		<optional>
			<attribute name="base">
				<data type="anyURI"/>
			</attribute>
		</optional>
	</define>
	
	<define name="a.identifier">
		<optional>
			<attribute name="identifier"/>
		</optional>
	</define>
	
	<define name="a.parent">
		<optional>
			<attribute name="parent">
				<data type="IDREFS"/>
			</attribute>
		</optional>
	</define>
	
	<define name="a.relator">
		<optional>
			<attribute name="relator"/>
		</optional>
	</define>
	
	<define name="a.link.xpointer">
		<optional>
			<attribute name="xpointer"/>
		</optional>
	</define>
	
	<!-- display attributes -->
	
	<define name="a.align">
		<attribute name="align">
			<choice>
				<value>left</value>
				<value>right</value>
				<value>center</value>
				<value>justify</value>
				<value>char</value>
			</choice>
		</attribute>
	</define>
	
	<define name="a.valign">
		<attribute name="valign">
			<choice>
				<value>top</value>
				<value>middle</value>
				<value>bottom</value>
			</choice>
		</attribute>
	</define>
	
	<define name="a.render">
		<optional>
			<attribute name="render">
				<choice>
					<value>altrender</value>
					<value>bold</value>
					<value>bolddoublequote</value>
					<value>bolditalic</value>
					<value>boldsinglequote</value>
					<value>boldsmcaps</value>
					<value>boldunderline</value>
					<value>doublequote</value>
					<value>italic</value>
					<value>nonproport</value>
					<value>singlequote</value>
					<value>smcaps</value>
					<value>sub</value>
					<value>super</value>
					<value>underline</value>
				</choice>
			</attribute>
		</optional>
	</define>
	
	<define name="a.label">
		<optional>
			<attribute name="label"/>
		</optional>
	</define>
	
	<!-- date attributes -->
	
	<define name="am.dates.calendar">
		<optional>
			<attribute name="calendar">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>
	
	<define name="am.dates.era">
		<optional>
			<attribute name="era">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>
	
	<define name="am.date.normal">
		<optional>
			<attribute name="normal">
				<data type="token"/>
			</attribute>
		</optional>
	</define>
	
	<define name="a.lastdatetimeverified">
		<attribute name="lastdatetimeverified">
			<data type="token"/>
		</attribute>
	</define>
	
	<define name="a.datechar">
		<optional>
			<attribute name="datechar"/>
		</optional>
	</define>
	
	<define name="a.certainty">
		<optional>
			<attribute name="certainty"/>
		</optional>
	</define>
	
	<!-- typing attributes -->

	<define name="a.encodinganalog">
		<optional>
			<attribute name="encodinganalog"/>
		</optional>
	</define>
	
	<define name="a.level">
		<attribute name="level">
			<choice>
				<value>class</value>
				<value>collection</value>
				<value>file</value>
				<value>fonds</value>
				<value>item</value>
				<value>otherlevel</value>
				<value>recordgrp</value>
				<value>series</value>
				<value>subfonds</value>
				<value>subgrp</value>
				<value>subseries</value>
			</choice>
		</attribute>
	</define>
	
	<define name="a.localtype">
		<optional>
			<attribute name="localtype"/>
		</optional>
	</define>
	
	<define name="a.relatedencoding">
		<optional>
			<attribute name="relatedencoding"/>
		</optional>
	</define>
	
	<define name="a.unitdatetype">
		<optional>
			<attribute name="unitdatetype">
				<choice>
					<value>bulk</value>
					<value>inclusive</value>
				</choice>
			</attribute>
		</optional>
	</define>
	
	<!-- table attributes -->
	
	<define name="a.char">
		<attribute name="char"/>
	</define>
	
	<define name="a.charoff">
		<attribute name="charoff">
			<data type="NMTOKEN"/>
		</attribute>
	</define>
	
	<define name="a.colname">
		<attribute name="colname">
			<data type="NMTOKEN"/>
		</attribute>
	</define>
	
	<define name="a.colsep">
		<attribute name="colsep">
			<ref name="av.zeroorone"/>
		</attribute>
	</define>
	
	<define name="a.rowsep">
		<attribute name="rowsep">
			<ref name="av.zeroorone"/>
		</attribute>
	</define>
	
	<!-- other attributes -->
	
	<define name="a.coverage">
		<attribute name="coverage">
			<choice>
				<value>whole</value>
				<value>part</value>
			</choice>
		</attribute>
	</define>
	
	<define name="am.countrycode">
		<optional>
			<attribute name="countrycode">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>
	
	<define name="a.transliteration">
		<optional>
			<attribute name="transliteration">
				<data type="NMTOKEN"/>
			</attribute>
		</optional>
	</define>

	<!-- ATTRIBUTE VALUE LISTS -->
	
	<define name="av.yesorno">
		<choice>
			<value>yes</value>
			<value>no</value>
		</choice>
	</define>

	<define name="av.zeroorone">
		<choice>
			<value>0</value>
			<value>1</value>
		</choice>
	</define>

</grammar>
