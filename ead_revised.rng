	<!-- Patterns from EAC-CPF for <control> -->

	<define name="maintenanceAgency">
		<element name="maintenanceAgency">
			<ref name="a.common"/>
			<optional>
				<element name="agencyCode">
					<optional>
						<ref name="a.common"/>
					</optional>
					<ref name="m.data.standardRepository"/>
				</element>
			</optional>
			<zeroOrMore>
				<element name="otherAgencyCode">
					<ref name="a.common"/>
					<ref name="a.localType"/>
					<data type="NMTOKEN"/>
				</element>
			</zeroOrMore>
			<element a:id="agencyName" name="agencyName">

				<text/>
			</element>
			<optional>
				<ref name="e.descriptiveNote"/>
			</optional>
		</element>
	</define>

	<define name="m.data.standardRepository">
		<data type="token">
			<a:documentation>This pattern represents the character pattern requirements of ISIL (ISO
				15511). Values that conform to the pattern are not, strictly speaking, conformant to
				ISIL, as all repository codes must be registered with a national authority and the
				national authority must be registered wth the official ISIL registering authority,
				the Royal Library of Belgium (http://biblstandard.dk/isil/).</a:documentation>
			<param name="pattern"
				>(([A-Z]{2})|([a-zA-Z]{1})|([a-zA-Z]{3,4}))(-[a-zA-Z0-9:/\-]{1,11})</param>
		</data>
	</define>


	<define name="maintenanceHistory">
		<ref name="a.common"/>
		<oneOrMore>
			<element a:id="maintenanceEvent" name="maintenanceEvent">
				<ref name="a.common"/>
				<element a:id="eventType" name="eventType">
					<ref name="a.common"/>
					<choice>
						<value>created</value>
						<value>revised</value>
						<value>deleted</value>
						<value>cancelled</value>
						<value>derived</value>
						<value>updated</value>
					</choice>
				</element>
				<element a:id="eventDateTime" name="eventDateTime">
					<ref name="a.common"/>
					<optional>
						<attribute a:id="standardDateTime" name="standardDateTime">
							<ref name="m.data.standardDateTime"/>
						</attribute>
					</optional>
					<text/>
				</element>
				<element a:id="agentType" name="agentType">
					<ref name="a.common"/>
					<choice>
						<value>human</value>
						<value>machine</value>
					</choice>
				</element>
				<element a:id="agent" name="agent">
					<ref name="a.common"/>
					<text/>
				</element>
				<optional>
					<element a:id="eventDesccription" name="eventDescription">
						<ref name="a.common"/>
						<text/>
					</element>
				</optional>
			</element>
		</oneOrMore>

	</define>



	<define name="e.languageDeclaration">
		<element a:id="languageDeclaration" name="languageDeclaration">
			<ref name="a.common"/>
			<ref name="e.language"/>
			<ref name="e.script"/>
			<optional>
				<ref name="e.descriptiveNote"/>
			</optional>
		</element>
	</define>







	<define name="e.sources">
		<element name="sources">
			<ref name="a.common"/>
			<optional>
				<attribute a:id="xmlBase" name="xml:base">
					<data type="anyURI"/>
				</attribute>
			</optional>
			<oneOrMore>
				<element name="source">
					<ref name="a.common"/>
					<optional>
						<ref name="a.lastDateTimeVerified"/>
					</optional>
					<optional>
						<ref name="simpleLink"/>
					</optional>
					<optional>
						<element name="sourceEntry">
							<optional>
								<ref name="m.entryLanguageAttributes"/>
							</optional>
							<text/>
						</element>
					</optional>
					<optional>
						<choice>
							<ref name="e.objectXMLWrap"/>
							<ref name="e.objectBinWrap"/>
						</choice>
					</optional>
					<optional>
						<ref name="e.descriptiveNote"/>
					</optional>
				</element>
			</oneOrMore>
		</element>
	</define>












	<define name="e.conventionDeclaration">
		<element a:id="conventionDeclaration" name="conventionDeclaration">
			<ref name="a.common"/>
			<optional>
				<ref name="e.abbreviation"/>
			</optional>
			<ref name="e.citation"/>
			<optional>
				<ref name="e.descriptiveNote"/>
			</optional>
		</element>
	</define>

	<define name="e.abbreviation">
		<element a:id="abbreviation" name="abbreviation">
			<ref name="a.common"/>
			<data type="token"/>
		</element>
	</define>

	<define name="e.citation">
		<element a:id="citation" name="citation">
			<ref name="a.common"/>
			<optional>
				<ref name="simpleLink"/>
			</optional>
			<optional>
				<ref name="a.lastDateTimeVerified"/>
			</optional>
			<ref name="m.mixedContent"/>
		</element>
	</define>
	<define name="m.mixedContent">
		<zeroOrMore>
			<choice>
				<text/>
				<ref name="e.span"/>
			</choice>
		</zeroOrMore>
	</define>
	<define name="e.span">
		<optional>
			<element a:id="span" name="span">
				<ref name="a.common"/>
				<ref name="a.localType"/>
				<optional>
					<attribute a:id="style" name="style">
						<text/>
					</attribute>
				</optional>
				<text/>
			</element>
		</optional>
	</define>
	<define name="e.localControl">
		<element a:id="localControl" name="localControl">
			<ref name="a.common"/>
			<ref name="a.localType"/>
			<optional>
				<ref name="e.term"/>
			</optional>
			<optional>
				<choice>
					<ref name="e.dateSingle"/>
					<ref name="e.dateRange"/>
				</choice>
			</optional>
		</element>
	</define>
	<define name="e.term">
		<element name="term">
			<ref name="a.common"/>
			<optional>
				<ref name="m.entryLanguageAttributes"/>
			</optional>
			<optional>
				<ref name="a.lastDateTimeVerified"/>
			</optional>
			<optional>
				<attribute name="source">
					<data type="string"/>
				</attribute>
			</optional>
			<text/>
		</element>
	</define>
	<define name="e.localTypeDeclaration">
		<element a:id="localTypeDeclaration" name="localTypeDeclaration">
			<ref name="a.common"/>
			<optional>
				<ref name="e.abbreviation"/>
			</optional>
			<ref name="e.citation"/>
			<optional>
				<ref name="e.descriptiveNote"/>
			</optional>
		</element>
	</define>
	<!--TC; 2012-06-13: create pattern for <emph> so it can be referenced from multiple patterns -->
	<define name="emph">
		<element name="emph">
			<optional>
				<attribute name="render">
					<choice>
						<ref name="av.render"/>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="id">
					<data type="ID"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="altRender"/>
			</optional>
			<!-- TC; 2013-06-13: permit only text -->
			<zeroOrMore>
				<choice>
					<text/>
					<!-- <ref name="m.phrase.basic"/> -->
				</choice>
			</zeroOrMore>
		</element>
	</define>

	<!-- TC 2012-10-10: added relations -->
	<define name="m.relation">
		<element name="relation">
			<optional>
				<choice>
					<element name="relationDate">
						<text/>
					</element>
					<element name="dateSet">
						<ref name="a.common"/>
						<optional>
							<ref name="a.localType"/>
						</optional>
						<choice>
							<element name="relationDate">
								<text/>
							</element>
							<ref name="m.relationDateRange"/>
						</choice>
						<oneOrMore>
							<choice>
								<element name="relationDate">
									<text/>
								</element>
								<ref name="m.relationDateRange"/>
							</choice>
						</oneOrMore>
					</element>
					<ref name="m.relationDateRange"/>
				</choice>
			</optional>
			<oneOrMore>
				<element  name="relationEntry">
					<ref name="a.common"/>
					<optional>
						<ref name="a.localType"/>
					</optional>
					<optional>
						<ref name="m.entryLanguageAttributes"/>
					</optional>
					<text/>
				</element></oneOrMore>
			<optional>
				<element  name="descriptiveNote">
					<ref name="a.common"/>
					<oneOrMore>
						<ref name="p"/>
					</oneOrMore>
				</element>
				
			</optional>
			<optional>
				<element a:id="placeEntry" name="placeEntry">
					<ref name="a.common"/>
					<optional>
						<ref name="m.entryLanguageAttributes"/>
					</optional>
					<optional>
						<ref name="a.localType"/>
					</optional>
					<optional>
						<attribute a:id="countryCode" name="countryCode">
							<choice>
								<data type="string">
									<param name="pattern">[A-Z]{2}</param>
								</data>
								<data type="string">
									<param name="pattern">[A-Z]{4}</param>
								</data>
							</choice>
						</attribute>
					</optional>
					<optional>
						<ref name="a.vocabularySource"/>
					</optional>
					<optional>
						<attribute a:id="accuracy" name="accuarcy">
							<text/>
						</attribute>
					</optional>
					<optional>
						<ref name="m.geographicalCoordinates"/>
					</optional>
					<text/>
				</element>
			</optional>
		</element>
	</define>
	<define name="m.relationDateRange">
		<element name="relationDateRange">
			<zeroOrMore>
				<choice>
					<element name="relationDate">
						<ref name="a.common"/>
						<optional>
							<ref name="a.localType"/>
						</optional>
						<text/>
					</element>
					<optional>
						<element name="fromDate">
							<ref name="a.common"/>
							<element name="relationDate">
								<text/>
							</element>
						</element>
					</optional>
					<optional>
						<element name="toDate">
							<ref name="a.common"/>
							<element name="relationDate">
								<text/>
							</element>
						</element>
					</optional>
				</choice>
			</zeroOrMore>
		</element>
	</define>
</grammar>
