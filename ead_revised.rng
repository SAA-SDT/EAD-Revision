							<ref name="p"/>
						</choice>
					</oneOrMore>
				</element>
			</optional>
			<optional>
				<element name="seriesStmt">
					<ref name="a.common"/>
					<optional>
						<attribute name="encodingAnalog"/>
					</optional>
					<oneOrMore>
						<choice>
							<ref name="titleProper"/>
							<ref name="num"/>
							<ref name="p"/>
						</choice>
					</oneOrMore>
				</element>
			</optional>
			<optional>
				<element name="noteStmt">
					<ref name="a.common"/>
					<optional>
						<attribute name="encodingAnalog"/>
					</optional>
					<oneOrMore>
						<ref name="note"/>
					</oneOrMore>
				</element>
			</optional>
			-->
			<ref name="e.languageDeclaration"/>
			<optional>
				<ref name="e.sources"/>
			</optional>
			<element a:id="publicationStatus" name="publicationStatus">
				<ref name="a.common"/>
				<choice>
					<value>inProcess</value>
					<value>approved</value>
				</choice>
			</element>
			<optional>
				<ref name="e.conventionDeclaration"/>
